<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    <title>Document</title>
</head>
<body>
    <table style="width: 500px; border-style: groove;">
        <thead>
            <tr>
              <th scope="col">ID</th>
              <th scope="col">HoTen</th>
              <th scope="col">Email</th>
              <td><a href="../sinhvien/them">Them</a></td>
            </tr>
        </thead>
        
        <% danhsach.rows.forEach(function(sv){

         %>
        
        <tbody>
            <tr>
              <th scope="row"><%= sv.id %></th>
              <td><%= sv.email %></td>
              <td><%= sv.hoten %></td>
              <td>
                <a href="/sinhvien/sua/<%= sv.id %>">Sua</a>
                <a  onclick="return confirm('Are you sure?')" href="/sinhvien/xoa/<%= sv.id %>">Xoa</a>
            </td>
            </tr>
            
          </tbody>
          <% }); %>
          
    </table>
</body>
</html>